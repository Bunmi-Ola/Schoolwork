        <Button x:Name="DisplayButton" Content="Button" HorizontalAlignment="Left" Margin="254,281,0,0" VerticalAlignment="Top" Width="117" Height="28" Click="DisplayButton_Click"/>
        <TextBox x:Name="diceSize" HorizontalAlignment="Left" Height="25" Margin="215,14,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="156"/>
        <TextBox x:Name="NumberofRolls" HorizontalAlignment="Left" Height="25" Margin="215,43,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="156"/>
        <TextBox x:Name="Outputbox" HorizontalAlignment="Left" Height="197" Margin="85,74,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="286"/>

    </Grid>
</Window>
